<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baiyang.oms.modular.business.dao.CountryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.baiyang.oms.modular.business.model.Country">
        <id column="id" property="id" />
        <result column="country_ename" property="countryEname" />
        <result column="country_cname" property="countryCname" />
        <result column="country_code" property="countryCode" />
        <result column="tenant_id" property="tenantId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, country_ename AS countryEname, country_cname AS countryCname, country_code AS countryCode, tenant_id AS tenantId
    </sql>

</mapper>
